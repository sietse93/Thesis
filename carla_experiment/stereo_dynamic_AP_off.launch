<launch>
  <arg name='rosbag_fname' default=''/>
  <param name="rosbag_fname" value="$(arg rosbag_fname)"/>
  <rosparam file="$(find carla_experiment)/config/stereo_dynamic.yaml" command="load" />
  <param name="carla_autopilot" type="boolean" value="False" />
  <node pkg="carla_experiment" name="carla_client" type="client_experiment.py" output="screen"/>
  <node pkg="carla_experiment" name="groundtruth_log" type="log_groundtruth.py" output="screen"/>
</launch>
